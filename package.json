{
  "name": "backend",
  "version": "1.0.0",
  "description": "UAACAI backend",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "start": "node server.js",
    "start:prod": "npm run build && node dist/server.js",
    "dev": "nodemon server.js",
    "dev:ts": "concurrently \"npx tsc -w\" \"nodemon --exec ts-node src/index.ts\"",
    "seed": "node prisma/seed.js",
    "seed:ts": "ts-node prisma/seed.ts",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "postprisma:generate": "shx cp -r node_modules/.prisma/client/index.d.ts ../client/src/types/prismaTypes.d.ts",
    "lint": "eslint . --ext .js,.ts",
    "test": "echo \"no tests yet\" && exit 0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/genai": "^1.8.0",
    "@prisma/client": "^6.19.2",
    "agenda": "^5.0.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.9.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.19.0",
    "multer": "^2.0.2",
    "resend": "^4.7.0",
    "slugify": "^1.6.6",
    "streamifier": "^0.1.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "nodemon": "^3.1.11",
    "prisma": "^6.19.2",
    "rimraf": "^5.0.10",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
